{
    "builds": [
      {
        "src": "src",
        "use": "@vercel/static",
        "config": {
          "target": "serverless",
          "framework": "static",
          "nodeVersion": "16",
          "buildOutputDirectory": "dist",
          "env": {
            "NODE_ENV": "production"
          },
          "customCommands": {
            "postBuild": "npm run postbuild"
          },
          "cache": {
            "paths": ["node_modules"],
            "npm": "npm --cache /tmp/.npm"
          }
        }
      }
    ]
  }
