{
    "builds": [
      {
        "src": "src",
        "use": "@vercel/static",
        "config": {
          "target": "serverless",
          "framework": "static",
          "nodeVersion": "18",
          "buildOutputDirectory": "dist",
          "env": {
            "NODE_ENV": "production"
          },
          "customCommands": {
            "postBuild": "npm run build"
          }
        }
      }
    ]
  }